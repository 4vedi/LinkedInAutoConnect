{
    "name": "Auto Connect",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "connect.png"
    },
    "icons": {
        "128": "connect.png"
    },
    "permissions": ["activeTab"],
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ]
}